{
    "name": "getarabictimepackage/arabic-time",
    "description": "A Laravel package to convert time to Arabic time periods.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yacoub Al-Haidari",
            "email": "yacoub@yacoubalhaidari.com"
        }
    ],
   "require": {
        "php": ">=7.3",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "nesbot/carbon": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "GetArabicTimePackage\\ArabicTime\\": "src/"
        }
